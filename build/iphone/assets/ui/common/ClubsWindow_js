function ClubsWindow(e,t,i,n){for(var o=Ti.UI.createView({backgroundColor:"#e2e2e2",top:n,bottom:60}),a=Ti.UI.createTableView({height:"auto",top:0}),r=[],l=0,s=0;s<=t.length-1;s++){if(l%2==0)var d=Ti.UI.createTableViewRow({city:t[s].city,height:"auto",selectionStyle:"none",index:s,bottom:10});else var d=Ti.UI.createTableViewRow({city:t[s].city,height:"auto",selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});var u=Ti.UI.createLabel({text:t[s].city,textAlign:"left",height:20,top:10,left:10,font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}});d.add(u);var w=Ti.UI.createLabel({text:t[s].leader,textAlign:"left",left:10,top:31,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});d.add(w);var c=46,m=10;if("NA"!=t[s].phone){var T=Ti.UI.createLabel({text:t[s].phone,textAlign:"left",left:10,top:c,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});d.add(T),c+=15}if("NA"!=t[s].email){var g=Ti.UI.createLabel({text:t[s].email,textAlign:"left",index:s,left:10,top:c,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});d.add(g),c+=15}if("NA"!=t[s].web){var b=Ti.UI.createLabel({text:t[s].web,textAlign:"left",left:10,top:c,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});d.add(b),c+=20}if("NA"!=t[s].phone){var f=Ti.UI.createButton({backgroundImage:"call.png",width:35,height:35,top:c,left:m,buttonid:s});m+=40,d.add(f),f.addEventListener("click",function(e){var n=t[e.source.buttonid].phone.replace(/(\|H: |C: |W: )/gm,"");n=n.replace(/(-)/gm,""),n=n.replace(/(H: )/gm,""),Titanium.Platform.openURL("tel:"+n),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].phone,value:1})})}if("NA"!=t[s].email){var h=Ti.UI.createButton({backgroundImage:"mail.png",width:35,height:35,top:c,left:m,buttonid:s});m+=40,h.addEventListener("click",function(e){var n=Ti.UI.createEmailDialog();n.toRecipients=[t[e.source.buttonid].email];var o=Ti.Filesystem.getFile("cricket.wav");n.addAttachment(o),n.open(),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].email,value:1})}),d.add(h)}if("NA"!=t[s].web){var v=Ti.UI.createButton({backgroundImage:"web.png",width:35,height:35,top:c,left:m,buttonid:s});v.addEventListener("click",function(e){new WebView(t[e.source.buttonid].web),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].web,autoLink:Titanium.UI.AUTOLINK_URLS,value:1})}),d.add(v)}l++,r.push(d)}return r=addRows(s,r,!0),a.setData(r),o.add(a),o}function addRows(e,t,i){if(1==e&&1==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n);var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",bottom:10});t.push(n);var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}else if(1==e&&0==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}else if(2==e&&1==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",bottom:10});t.push(n);var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}else if(3==e&&1==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}return t}var GetFeed=require("ui/common/GetFeed"),WebView=require("ui/common/WebView"),ApplicationWindow=require("ui/common/ApplicationWindow"),StaticAd=require("ui/common/StaticAd"),Feed=require("ui/common/Feed");module.exports=ClubsWindow;