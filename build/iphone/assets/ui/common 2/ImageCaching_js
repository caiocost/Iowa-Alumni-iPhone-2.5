function ImageCaching(e,t){var i=t.split("/");i=i[i.length-1];var o=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,i),a="";if(o.exists())a=o.nativePath,o=null;else{var n=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);n.exists()||n.createDirectory();var r=Ti.Network.createHTTPClient();r.onload=function(){200==r.status&&(o.write(r.responseData),a=o.nativePath,o=null)},r.open("GET",t),r.send()}return a}module.exports=ImageCaching;