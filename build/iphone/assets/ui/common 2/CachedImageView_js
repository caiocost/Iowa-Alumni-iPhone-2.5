function CachedImageView(e,t,i){var o=t.split("/");o=o[o.length-1];var a=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,o);if(a.exists())i.image=a.nativePath,a=null;else{var n=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);n.exists()||n.createDirectory();var r=Ti.Network.createHTTPClient();r.onload=function(){200==r.status&&(a.write(r.responseData),i.image=a.nativePath,a=null)},r.open("GET",t),r.send()}}module.exports=CachedImageView;