function DateObject(e){this.pubDate=e}DateObject.prototype.dateString=function(){var e=new Date(this.pubDate),t=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"),i=e.getDate();10>i&&(i="0"+i);var o=e.getMonth(),a=t[o]+" "+i,n="AM",r=e.getHours(),l=e.getMinutes();r>=12&&(n="PM"),r>12&&(r-=12),10>l&&(l="0"+l.toString()),10>r&&(r="0"+r.toString());var d=r+":"+l+n;return a+" "+d},DateObject.prototype.magazineDateString=function(){var e=new Date(this.pubDate),t=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"),i=e.getMonth(),o=t[i];return o+" "+e.getYear()},DateObject.prototype.prettyDate=function(){var e=["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],t=new Date(this.pubDate),i=(new Date,(new Date).getTime()-t.getTime()),o=Math.floor(i/86400);if(isNaN(o)||0>o)return"";if(o>=31){var a=t.getFullYear(),n=e[t.getMonth()],r=t.getMonth()+1;10>r&&(r="0"+r);var l=t.getDate();return 10>l&&(l="0"+l),l+" "+n+" "+a}return 0==o&&(60>i&&"just now"||120>i&&"1 minute ago"||3600>i&&Math.floor(i/60)+" minutes ago"||7200>i&&"1 hour ago"||86400>i&&"about "+Math.floor(i/3600)+" hours ago")||1==o&&"Yesterday"||7>o&&o+" days ago"||31>o&&Math.ceil(o/7)+" week"+(1==Math.ceil(o/7)?"":"s")+" ago"},module.exports=DateObject;