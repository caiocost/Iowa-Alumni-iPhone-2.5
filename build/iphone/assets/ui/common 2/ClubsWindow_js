function ClubsWindow(e,t,i,o){for(var a=Ti.UI.createView({backgroundColor:"#e2e2e2",top:o,bottom:60}),n=Ti.UI.createTableView({height:"auto",top:0}),r=[],l=0,d=0;d<=t.length-1;d++){if(l%2==0)var c=Ti.UI.createTableViewRow({city:t[d].city,height:"auto",selectionStyle:"none",index:d,bottom:10});else var c=Ti.UI.createTableViewRow({city:t[d].city,height:"auto",selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});var s=Ti.UI.createLabel({text:t[d].city,textAlign:"left",height:20,top:10,left:10,font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}});c.add(s);var u=Ti.UI.createLabel({text:t[d].leader,textAlign:"left",left:10,top:31,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});c.add(u);var g=46,h=10;if("NA"!=t[d].phone){var m=Ti.UI.createLabel({text:t[d].phone,textAlign:"left",left:10,top:g,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});c.add(m),g+=15}if("NA"!=t[d].email){var w=Ti.UI.createLabel({text:t[d].email,textAlign:"left",index:d,left:10,top:g,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});c.add(w),g+=15}if("NA"!=t[d].web){var f=Ti.UI.createLabel({text:t[d].web,textAlign:"left",left:10,top:g,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});c.add(f),g+=20}if("NA"!=t[d].phone){var p=Ti.UI.createButton({backgroundImage:"call.png",width:35,height:35,top:g,left:h,buttonid:d});h+=40,c.add(p),p.addEventListener("click",function(e){var o=t[e.source.buttonid].phone.replace(/(\|H: |C: |W: )/gm,"");o=o.replace(/(-)/gm,""),o=o.replace(/(H: )/gm,""),Titanium.Platform.openURL("tel:"+o),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].phone,value:1})})}if("NA"!=t[d].email){var b=Ti.UI.createButton({backgroundImage:"mail.png",width:35,height:35,top:g,left:h,buttonid:d});h+=40,b.addEventListener("click",function(e){var o=Ti.UI.createEmailDialog();o.toRecipients=[t[e.source.buttonid].email];var a=Ti.Filesystem.getFile("cricket.wav");o.addAttachment(a),o.open(),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].email,value:1})}),c.add(b)}if("NA"!=t[d].web){var T=Ti.UI.createButton({backgroundImage:"web.png",width:35,height:35,top:g,left:h,buttonid:d});T.addEventListener("click",function(e){new WebView(t[e.source.buttonid].web),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].web,autoLink:Titanium.UI.AUTOLINK_URLS,value:1})}),c.add(T)}l++,r.push(c)}return r=addRows(d,r,!0),n.setData(r),a.add(n),a}function addRows(e,t,i){if(1==e&&1==i){var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(o);var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",bottom:10});t.push(o);var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(o)}else if(1==e&&0==i){var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(o)}else if(2==e&&1==i){var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",bottom:10});t.push(o);var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(o)}else if(3==e&&1==i){var o=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(o)}return t}var GetFeed=require("ui/common/GetFeed"),WebView=require("ui/common/WebView"),ApplicationWindow=require("ui/common/ApplicationWindow"),StaticAd=require("ui/common/StaticAd"),Feed=require("ui/common/Feed");module.exports=ClubsWindow;