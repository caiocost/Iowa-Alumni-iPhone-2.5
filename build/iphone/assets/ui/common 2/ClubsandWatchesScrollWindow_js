function ClubsandWatchesScrollWindow(e,t,i,o){for(var a=0,n=Ti.UI.createView({backgroundColor:"#fff"}),r=60,l=e[0].state,d=0,c=Ti.UI.createImageView({image:"slide_right.png",bottom:3,right:5,width:54,height:54,zIndex:5}),s=Ti.UI.createImageView({image:"slide_left.png",bottom:3,left:5,width:54,height:54,zIndex:5}),u=["Iowa Clubs","Game Watch Locations"],g=["dots2_1.png","dots2_2.png"],h=[],m=0;2>m;m++){var w=Titanium.UI.createView({}),f=Ti.UI.createLabel({text:u[m],font:{fontFamily:"HelveticaNeue-Light",fontSize:20,fontWeight:"bold"},top:15}),p=Ti.UI.createImageView({image:g[m],top:45,height:10});w.add(f),w.add(p),h[m]=w}var b=Titanium.UI.createScrollableView({width:Ti.UI.FILL,height:r,views:h,bottom:0,backgroundColor:"#ccc",contentWidth:Ti.UI.FILL,contentHeight:100});n.add(c),n.add(s),c.hide(),s.hide();var T=new ClubsWindow(e,t,i,d);n.add(T),n.add(b);var v=[n,T,o];b.addEventListener("scrollend",function(e){0==e.currentPage&&1==a?(v[0].remove(v[2]),a=0):1==e.currentPage&&0==a&&(v[0].add(v[2]),a=1)}),b.addEventListener("scroll",function(){s.hide(),c.hide()}),b.addEventListener("touchstart",function(){0==a?s.show():c.show()}),b.addEventListener("touchend",function(){0==a?s.hide():c.hide()});var I=new NavigateWindow(l,n);return I}var NavigateWindow=require("ui/common/NavigateWindow"),ClubsWindow=require("ui/common/ClubsWindow"),GameWatchWindow=require("ui/common/GameWatchWindow");module.exports=ClubsandWatchesScrollWindow;