function ApplicationWindow(e,t){var i=(Ti.UI.createView({backgroundColor:"#000",top:0,height:20}),Ti.UI.createWindow({title:e,backgroundColor:"#e2e2e2",width:Ti.Platform.displayCaps.platformWidth,left:0,barImage:"navbar.png",translucent:!1,moving:!1,axis:0})),o=Ti.UI.createButton({backgroundImage:"newmenubutton.png",backgroundSelectedImage:"newmenubuttonselected.png",title:"",height:22,width:37,toggle:!1});i.setLeftNavButton(o),i.add(t);var a=Titanium.UI.iOS.createNavigationWindow({window:i});return o.addEventListener("click",function(){a.fireEvent("menuClick")}),a.addEventListener("swipeToggle",function(){a.fireEvent("menuClick")}),a.addEventListener("swipe",function(){a.fireEvent("menuClick")}),a.addEventListener("swipeListen",function(){a.fireEvent("menuClick")}),a}module.exports=ApplicationWindow;