function MasterView(e,t,i){function o(){l(),a()}function a(){u.setContentInsets({top:60},{animated:!0}),u.reloading=!1,u.updateLabelText("Refreshing..."),setTimeout(n,700)}function n(){u.setContentInsets({top:0},{animated:!0}),u.updateDateText("Last Updated: "+(new FormatDate).getDate()),u.hideActInd(),u.updateLabelText("Pull down to refresh...")}function r(o){if("[object Array]"===Object.prototype.toString.apply(o)){for(var a=[],n=[],r=!1,l=0,c=0,g=0,h=0,m=new GetFeed(d.adFeed()),f="",w=0;w<o.length;w++){var p=new Post(o[w]);if(0==w&&e==d.iowaInsiderFeed()){var b=new IIBIntroRow;a.push(b)}if(0==w&&e==d.magazineFeed()){var b=new IAMIntroRow;a.push(b)}if(0!=c&&c%3==0&&3>h&&e==d.iowaInsiderFeed()){var b=new Ad(m[h+3],i,t);a.push(b),h++}if(0!=c&&c%3==0&&3>h&&e==d.magazineFeed()){var b=new Ad(m[h+6],i,t);a.push(b),h++}if(null!=p.imageheight&&p.imageheight>150&&p.imageheight<300&&0==r&&e!=d.eventsFeed()){var b=new FeatureRow(p,i,t);r=!0,b.addEventListener("swipe",function(){s.fireEvent("swipeToggle")}),a.push(b)}else if(e==d.eventsFeed()){if(0==c||f!=p.pubDate&&0!=c){var T=new HeaderRow(p,i,t);if(0!=g&&g%3==0&&3>h){var b=new Ad(m[h],i,t);a.push(b),h++,3==h&&(h=0)}a.push(T),g++}var b=new SingleRow(p,i,t);a.push(b)}else{var b=new Row(p,i,t);l>=1?(n.push(b),a.push(new PostGroup(n)),n=[],l=0,r=!1):(n.push(b),l++)}c++,f=p.pubDate}u.setData(a)}}function l(){c.loadRssFeed({success:function(e){r(e)}})}var d=new Feed,c=new RSS(e),s=Ti.UI.createView({backgroundColor:"#fff"}),u=new PostTable;if(u.addEventListener("scroll",function(e){var t=e.contentOffset.y;-65>t&&!u.pulling&&!u.eloading?(u.pulling=!0,u.updateLabelText("Release to refresh...")):t>-65&&0>t&&u.pulling&&!u.reloading&&(u.pulling=!1,u.updateLabelText("Pull down to refresh..."))}),u.addEventListener("dragEnd",function(){u.pulling&&!u.reloading&&(u.reloading=!0,u.pulling=!1,u.showActInd(),u.setContentInsets({top:60},{animated:!0}),u.scrollToTop(-60,!0),o(),i.trackEvent({category:"Refreshing  Window",action:"refresh",label:"Refreshing "+t+"'s Window",value:1}))}),l(),u.bottom=70,s.add(u),e==d.iowaInsiderFeed())var g=new StaticAd(13,350,i,t);else if(e==d.magazineFeed())var g=new StaticAd(12,350,i,t);else var g=new StaticAd(10,350,i,t);return s.add(g),s}var Post=require("ui/common/Post"),FeatureRow=require("ui/common/FeatureRow"),Row=require("ui/common/Row"),SingleRow=require("ui/common/SingleRow"),HeaderRow=require("ui/common/HeaderRow"),IIBIntroRow=require("ui/common/IIBIntroRow"),IAMIntroRow=require("ui/common/IAMIntroRow"),PostGroup=require("ui/common/PostGroup"),PostTable=require("ui/common/PostTable"),Ad=require("ui/common/Ad"),GetFeed=require("ui/common/GetFeed"),FormatDate=require("ui/common/FormatDate"),RSS=require("services/rss"),WebView=require("ui/common/WebView"),StaticAd=require("ui/common/StaticAd");Feed=require("ui/common/Feed"),module.exports=MasterView;