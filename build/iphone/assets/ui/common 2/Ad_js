function Ad(e,t,i){var o=Ti.UI.createTableViewRow({hasChild:!0,height:70,padding:0,top:0,bottom:0,layout:"vertical",backgroundColor:"e2e2e2",borderRadius:.5,selectionStyle:"none"});o.rightImage=null,o.backgroundSelectedImage=null,o.backgroundFocusImage=null;var a=Titanium.UI.createView({backgroundColor:"transparent",height:300,width:300,left:10,top:-5,bottom:0,padding:0,borderRadius:5}),n=Ti.UI.createImageView({image:e.ad,defaultImage:"loader480x120.png",width:300,height:70,hires:!0,top:10});return a.add(n),o.add(a),o.addEventListener("click",function(){new WebView(e.link),t.trackEvent({category:"Ads",action:"click",label:"An Ad in the "+i+"'s Window - "+e.ad,value:1})}),o}var WebView=require("ui/common/WebView");module.exports=Ad;